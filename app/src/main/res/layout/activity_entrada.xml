<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Entrada">

    <com.mikhaellopez.circularfillableloaders.CircularFillableLoaders
        android:id="@+id/logo"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/yo"
        app:cfl_border="true"
        app:cfl_border_width="4dp"
        app:cfl_progress="85"
        app:cfl_wave_amplitude="0.09"
        app:cfl_wave_color="@color/h1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"/>

    <TextView
        android:id="@+id/texto_entrada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/christmaseve"
        android:text="@string/cargando"
        android:layout_below="@id/logo"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"
        android:textColor="@color/h1"/>

    <ImageView
        android:id="@+id/adorno1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="-30dp"
        app:srcCompat="@drawable/adorno1"
        android:contentDescription="@string/adorno" />
    <ImageView
        android:id="@+id/adorno2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="-30dp"
        app:srcCompat="@drawable/adorno2"
        android:contentDescription="@string/adorno"/>
    <ImageView
        android:id="@+id/adorno3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="-30dp"
        app:srcCompat="@drawable/adorno3"
        android:contentDescription="@string/adorno"/>
    <ImageView
        android:id="@+id/adorno4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="-30dp"
        app:srcCompat="@drawable/adorno4"
        android:contentDescription="@string/adorno"/>
    <ImageView
        android:id="@+id/adorno5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="-30dp"
        app:srcCompat="@drawable/adorno5"
        android:contentDescription="@string/adorno"/>
    <ImageView
        android:id="@+id/adorno6"
        android:layout_width="30dp"
